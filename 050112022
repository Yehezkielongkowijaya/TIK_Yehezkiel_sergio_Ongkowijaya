#include <iostream>

using namespace std;
int main(){
string namaSiswa[4] = {
    "anniko",
    "hanis",
    "samuel",
    "justin"
};
cout<<namaSiswa[0]<<endl<<namaSiswa[1]<<endl<<namaSiswa[2]<<endl<<namaSiswa[3]<<endl<<namaSiswa[4]<<endl;
for (int i = 0; i<4; i++){
    cout<<namaSiswa[i]<<endl;
}

return 0;
}
